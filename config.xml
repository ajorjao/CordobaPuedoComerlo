<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.kek" version="2.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>¿Puedo Comerlo?</name>
    <description>
        Aplicacion movil que permite escanear alimentos que pueden ser dañinos para ti o tu familia.
    </description>
    <author email="jorge.caullan.12@sansano.usm.cl" href="http://puedocomerlo.feriadesoftware.cl/">
        Equipo Zineu
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.1.1" />
     <platform name="android">
        <!--
            ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px
        -->
        <icon src="tamannos/36.png" density="ldpi" />
        <icon src="tamannos/48.png" density="mdpi" />
        <icon src="tamannos/72.png" density="hdpi" />
        <icon src="tamannos/96.png" density="xhdpi" />
        <icon src="tamannos/144.png" density="xxhdpi" />
        <icon src="tamannos/192.png" density="xxxhdpi" />
    </platform>
</widget>
